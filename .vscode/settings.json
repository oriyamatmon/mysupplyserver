{
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "eslint.autoFixOnSave": true,
  "editor.tabSize": 2,
  "jshint.nodePath": "./",
  "eslint.workingDirectories": [
    {
      "directory": "./",
      "changeProcessCWD": true
    }
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "deno.enable": false
}
